<?xml version="1.0" encoding="UTF-8"?>
<mule
	xmlns:tls="http://www.mulesoft.org/schema/mule/tls"
	xmlns:module-logger="http://www.mulesoft.org/schema/mule/module-logger"
	xmlns:error-handler-plugin="http://www.mulesoft.org/schema/mule/error-handler-plugin"
	xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:apikit="http://www.mulesoft.org/schema/mule/mule-apikit"
	xmlns:db="http://www.mulesoft.org/schema/mule/db"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:http="http://www.mulesoft.org/schema/mule/http"
	xmlns:os="http://www.mulesoft.org/schema/mule/os"
	xmlns:wsc="http://www.mulesoft.org/schema/mule/wsc"
	xmlns:secure-properties="http://www.mulesoft.org/schema/mule/secure-properties"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation=" http://www.mulesoft.org/schema/mule/mule-apikit http://www.mulesoft.org/schema/mule/mule-apikit/current/mule-apikit.xsd
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd http://www.mulesoft.org/schema/mule/core 
http://www.mulesoft.org/schema/mule/core/current/mule.xsd http://www.mulesoft.org/schema/mule/db http://www.mulesoft.org/schema/mule/db/current/mule-db.xsd 
http://www.mulesoft.org/schema/mule/os http://www.mulesoft.org/schema/mule/os/current/mule-os.xsd 
http://www.mulesoft.org/schema/mule/wsc http://www.mulesoft.org/schema/mule/wsc/current/mule-wsc.xsd
http://www.mulesoft.org/schema/mule/error-handler-plugin http://www.mulesoft.org/schema/mule/error-handler-plugin/current/mule-error-handler-plugin.xsd
http://www.mulesoft.org/schema/mule/secure-properties http://www.mulesoft.org/schema/mule/secure-properties/current/mule-secure-properties.xsd
http://www.mulesoft.org/schema/mule/module-logger http://www.mulesoft.org/schema/mule/module-logger/current/mule-module-logger.xsd
http://www.mulesoft.org/schema/mule/tls http://www.mulesoft.org/schema/mule/tls/current/mule-tls.xsd">
	<http:listener-config
		name="tgif-mobilepos-papi-httpListenerConfig">
		<http:listener-connection host="0.0.0.0"
			port="${secure::api.https.port}" protocol="HTTPS">
			<tls:context >
				<tls:key-store path="ssl/server.jks" keyPassword="${secure::keystore.password}" password="Fridays" />
			</tls:context>
		</http:listener-connection>
	</http:listener-config>
	<apikit:config name="tgif-mobilepos-papi-config"
		api="tgif-mobilepos-papi.raml"
		outboundHeadersMapName="outboundHeaders"
		httpStatusVarName="httpStatus" />
	<secure-properties:config
		name="Secure_Properties_Config" doc:name="Secure Properties Config"
		doc:id="77fc800c-73a6-462f-90ed-ad281854e78a"
		file="config/${mule.env}-config.yaml" key="${mule.key}">
		<secure-properties:encrypt
			algorithm="Blowfish" />
	</secure-properties:config>
	<db:config name="Database_Config_Depot"
		doc:name="Database Config"
		doc:id="a22595e0-d1f0-4100-991d-f4d4f026e472">
		<db:generic-connection
			url="${secure::database.depot.url}"
			driverClassName="${secure::database.driver}"
			user="${secure::database.depot.user}"
			password="${secure::database.depot.password}" />
	</db:config>
	<db:config name="Database_Config_Mule_Interface"
		doc:name="Database Config"
		doc:id="52dbe492-ac32-436d-a4fb-9430d2d655eb">
		<db:generic-connection
			url="${secure::database.muleInterface.url}"
			driverClassName="${secure::database.driver}"
			user="${secure::database.muleInterface.user}"
			password="${secure::database.muleInterface.password}" />
	</db:config>
	<os:object-store name="Object_store"
		doc:name="Object store" doc:id="37e7f8a6-b58b-4952-b71e-e792750684ef" />
	<wsc:config name="Web_Service_Consumer_Config"
		doc:name="Web Service Consumer Config"
		doc:id="db0c9a94-7f77-43a5-a868-8b741a2505d9">
		<wsc:connection wsdlLocation="wsdl\TGIServer.wsdl"
			service="Micros" port="BasicHttpBinding_ITGIServer"
			address='#["http://" ++ vars.storeIpProperties[0].serverIp ++ ":8086/TGIServer/TGIServer.svc"]'>
			<wsc:custom-transport-configuration>
				<wsc:http-transport-configuration
					requesterConfig="HTTP_Request_configuration_wsc" />
			</wsc:custom-transport-configuration>
		</wsc:connection>
	</wsc:config>
	<http:request-config
		name="HTTP_Request_configuration"
		doc:name="HTTP Request configuration"
		doc:id="9b7c4448-7e5e-45c4-856b-49963a0309f0">
		<http:request-connection
			host="#[vars.storeIpProperties[0].serverIp]" />
	</http:request-config>
	<http:request-config
		name="HTTP_Request_configuration_wsc"
		doc:name="HTTP Request configuration" requestStreamingMode="NEVER"
		doc:id="ee64ec23-5950-48ff-bff6-764755e095c9" />
	<error-handler-plugin:config
		name="Error_Handler_Plugin_Config"
		doc:name="Error Handler Plugin Config"
		doc:id="485d8342-fdef-4f29-872b-045366c0aeca" />
	<module-logger:config name="TGIF_Logger_Config"
		doc:name="TGIF Logger Config"
		doc:id="6f157497-ce4e-4841-8378-ce2c810601db"
		host="${secure::tgif.logger.database.host}"
		port="${secure::tgif.logger.database.port}"
		user="${secure::tgif.logger.database.user}"
		password="${secure::tgif.logger.database.password}"
		databaseName="${secure::tgif.logger.database.dbName}" />

</mule>
